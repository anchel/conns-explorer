{
  "name": "conns-explorer",
  "version": "1.1.19",
  "private": true,
  "description": "A Connectors Explorer",
  "author": "anchel",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "make": "vue-tsc --noEmit && vite build && electron-builder",
    "make:all": "vue-tsc --noEmit && vite build && electron-builder -mwl --arm64",
    "make:all:x64": "vue-tsc --noEmit && vite build && electron-builder -mwl --x64",
    "make:mas": "vue-tsc --noEmit && vite build && electron-builder -c electron-builder-mas.json5",
    "preview": "vite preview"
  },
  "dependencies": {
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "dayjs": "^1.11.13",
    "electron": "^32.2.7",
    "electron-builder": "^24.13.3",
    "element-plus": "^2.9.1",
    "less": "^4.2.1",
    "pinia": "^2.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.2.2",
    "uuid": "^11.0.3",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.0.26"
  }
}
